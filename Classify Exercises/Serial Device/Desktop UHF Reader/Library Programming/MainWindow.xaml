<Window x:Class="Library_Programming.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Library_Programming"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnDevice" Content="连接设备" HorizontalAlignment="Left" Margin="30,30,0,0" VerticalAlignment="Top" Width="100" Tag="0" FontSize="14" Click="btnDevice_Click"/>
        <Button x:Name="btnRead" Content="读取" HorizontalAlignment="Left" Margin="160,30,0,0" VerticalAlignment="Top" Width="75" Tag="0" FontSize="14" Click="btnRead_Click" IsEnabled="False"/>
        <ListBox x:Name="lsbEPC" HorizontalAlignment="Left" Height="300" Margin="30,75,0,0" VerticalAlignment="Top" Width="400"/>

    </Grid>
</Window>
